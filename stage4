rm -rf stage3
lsb_release -a 
echo "sleeping for 15 seconds"
echo "let's proceed to stage 4"

sudo sed -i 's/bullseye/bookworm/g' /etc/apt/sources.list

sudo apt update
sudo apt upgrade -y
sudo apt full-upgrade -y
sudo apt autoremove -y

echo "downloading stage 5"
wget https://github.com/elvicw/lazy-update/raw/refs/heads/main/stage5
chmod +x stage5

sudo reboot
